## Libraries
library(tidyverse)

library(lubridate)

## Workign with Pipes
library(dplyr)
filtered_vaccinetype <- VaccineTypes %>%
  filter(dose==1) %>%
  arrange(len)

library(dplyr)
filtered_vaccinetype <- VaccineTypes %>%
  filter(dose==1) %>%
  group_by(location) %>%
  arrange(len)


## Anscombe's Quartet Example
install.packages("Tmisc")
library(Tmisc)
data(Quartet)
View(Quartet)
